# sum of two numbers by taking their input
a = int(input(""))
b = int(input(""))
sum = a + b
print(sum)

# input side of a square and prints its area
a = int(input(""))
area = a * a
print(area)

# input two floating number and print their average
a = float(input(""))
b = float(input(""))
average = (a + b) / 2
print(average)

# input two int numbers a and b and print true if a is grater than or equal to b otherwise print false
a = int(input(""))
b = int(input(""))
print (a >= b)
